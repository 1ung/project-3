<style>
.block {
  display: inline-block;
}
</style>
<div class="block">
<h1> Patient History Taking </h1>
<%= form_with scope: :consult, url: consults_path, local: true do |form| %>
 
  <p>
    <%= form.label :user_id %><br>
    <%= form.text_field :user_id %>
  </p>

  <p>
    <%= form.label :history %><br>
    <%= form.text_area :history %>
  </p>

  <p>
    <%= form.label :prescription %><br>
    <%= form.text_area :prescription %>
  </p>

   <p>
    <%= form.label :mc %><br>
    <%= form.text_field :mc %>
  </p>

  <p>
    <%= form.label :doctor_id %><br>
    <%= form.text_field :doctor_id %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
</div>

<div class="block">
    <script>
      window.localStorage.setItem('debug', '* -engine* -socket* -RIE* *WARN* *ERROR*');

      if (window.antiglobal)
      {
        window.antiglobal('___browserSync___oldSocketIo', 'io', '___browserSync___', '__core-js_shared__');
        setInterval(window.antiglobal, 180000);
      }
    </script>
    <script async src='https://demo.mediasoup.org/mediasoup-demo-app.js'></script>

</div>