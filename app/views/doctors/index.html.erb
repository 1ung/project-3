<h1 class="center-align">Doctor Dashboard</h1>

<ul class="collection">
<% @patients.each do |patient| %>
    <li class="collection-item avatar z-depth-4">
    <i class="material-icons circle">perm_identity</i>
    <span>ID: <%= patient.id %></span>
    <p>Name: <%= link_to patient.first_name, patient_path(patient.id) %> <br>
    <%= link_to 'Consult', new_patient_consult_path(patient.id) %> </p>
    </li>

<% end %>
</ul>

<%= link_to 'Back', root_path %>
